{
  "name": "pinbot",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && sed -i '' 's/document\\.baseURI/self\\.location\\.href/g' dist/assets/worker-*.js && sed -i '' 's/document\\.currentScript&&document\\.currentScript.src||//g' dist/assets/worker-*.js",
    "models": "git lfs install && cd public/models/onnx && git sparse-checkout set quantized/sentence-transformers/all-MiniLM-L6-v2",
    "preview": "vite preview",
    "clone": "git clone --depth 1 --filter=blob:none --sparse https://huggingface.co/Xenova/transformers.js"
  },
  "dependencies": {
    "@floating-ui/react": "^0.23.1",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@tabler/icons-react": "^2.14.0",
    "@xenova/transformers": "github:klavinski/transformers.js",
    "framer-motion": "^10.12.4",
    "lucide-react": "^0.130.0",
    "pretendard": "^1.3.6",
    "react": "^18.2.0",
    "react-calendar": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-virtualized": "^9.22.5",
    "sqlite-wasm-http": "^1.1.1",
    "twemoji-colr-font": "^14.0.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "untitled-ui-icons": "^1.0.14",
    "vite-plugin-svgr": "^2.4.0",
    "voy-search": "^0.1.3",
    "webextension-polyfill": "^0.10.0",
    "wink-eng-lite-web-model": "^1.5.0",
    "wink-nlp": "^1.13.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.15",
    "@types/chrome": "^0.0.228",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-virtualized": "^9.21.21",
    "@types/serviceworker": "^0.0.67",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.4.21",
    "typescript": "^5.0.3",
    "vite": "^4.2.1"
  }
}
